{% if menu %}
	<ul class="menu-wrapper row justify-content-center">
		{% for item in items %}
			<li class="menu-link {{ item.classes | join(' ') }}">
				<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
				{% include "submenu.twig" with {'items': item.children} %}
			</li>
		{% endfor %}
	</ul>
{% endif %}
